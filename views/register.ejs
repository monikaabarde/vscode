    <form action="/register" method="POST">
    <div id="main">
        <div class="left">
            <div class="b">
                <h1>Welcome</h1>
            </div>
        </div>
        <div class="right">
            <h1>Your personal info</h1>
            <div class="container">
                <div class="slide1">
                    <div class="box">
                        <div class="box1">
                    <p>First Name</p>
                    <input type="text" name="fname" placeholder="Enter your First Name">
                    </div>
                    <div class="box1">
                        <p>Last Name</p>
                        <input type="text" name="lname" placeholder="Enter your Last Name">
                        </div>
                        <div class="box1">
                            <p>Email</p>
                           <input type="email" name="email" placeholder="Enter your Email">
                        </div>
                        <button>change Email ID</button>
                            <div class="box1">
                                <p>Mobile Number</p>
                                <input type="Number" name="mnum" placeholder="Enter your Mobile Number">
                            </div>
                            <button>change Mobile No.</button>
                                <div class="box1">
                                    <p>Date of birth</p>
                                    <input type="date" name="dob">
                                    </div>
                                    <div class="box1">
                                        <p>Whatsapp Number</p>
                                        <input type="number" name="wnum" placeholder="Enter Your Whatsapp Number">
                                        </div>
                </div>
                    
                </div>
                <div class="slide2">
                    
                    <div class="box">
                        
                    <div class="box1">
                        <p>Gender</p>
                        <select name="gender" >
                            <option value="none" selected>Gender</option>
                            <option value="male">Male</option>
                            <option value="female">Female</option>
                            <option value="other">other</option>
                        </select>
                        </div>
                        <div class="box1">
                            <p>State</p>
                            <select name="state">
                                <option value="none" selected>Select Your State</option>
                                <option value="Madhya Pradesh">Madhya Pradesh</option>
                                <option value="Uttar Pradesh">Uttar Pradesh</option>
                                <option value="Rajasthan">Rajasthan</option>
                            </select>
                            </div>
                            <div class="box1">
                                <p>District</p>
                                <select name="district">
                                    <option value="none" selected>Select Your District</option>
                                    <option value="Madhya Pradesh">Bhopal</option>
                                    <option value="Uttar Pradesh">jabalpur</option>
                                    <option value="Rajasthan">Gwalior</option>
                                </select>
                                </div>
                                <div class="box1">
                                    <p>Religion</p>
                                    <select name="religion">
                                        <option value="none" selected>Select Your Religion</option>
                                        <option value="Hindu">Hindu</option>
                                        <option value="Muslim">Muslim</option>
                                        <option value="other">other</option>
                                    </select>
                                    </div>
                                    <div class="box1">
                                        <h2>Education Information</h2>
                                        <br>
                                        <p>Present Class/Degree</p>
                                        <input type="text" name="class">
                                        </div>
                                        <div class="box1">
                                        <p>Course/Subject/Stream</p>
                                        <input type="text" name="course">
                                        </div>
                </div>
                </div>
                <div class="slide3">
                    
                    <div class="box">
                        <div class="box1">
                            <h2>Other Information</h2>
                            <br>
                    <p>Annual Family Income</p>
                   <input type="text" name="income">
                    </div>
                    <div class="box1">
                        <p>Physically Changed</p>
                        <select name="pc">
                            <option value="none" selected>Select Your Religion</option>
                            <option value="Hindu">Hindu</option>
                            <option value="Muslim">Muslim</option>
                            <option value="other">other</option>
                        </select>
                        </div>
                    
                        <div class="box1">
                           <button type="submit" >Save your information</button>
                           
                            </div>
                           
                            <div class="box1">
                                <br>
                                <h2>delete my account</h2>
                                <br>
                                <p>Are you sure you wish to leave us?</p>
                                <br>
                               <button>Delete My Account</button>
                </div>
